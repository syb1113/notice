#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/HP/Desktop/notice/node_modules/.pnpm/anywhere@1.6.0/node_modules/anywhere/bin/node_modules:/mnt/c/Users/HP/Desktop/notice/node_modules/.pnpm/anywhere@1.6.0/node_modules/anywhere/node_modules:/mnt/c/Users/HP/Desktop/notice/node_modules/.pnpm/anywhere@1.6.0/node_modules:/mnt/c/Users/HP/Desktop/notice/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/HP/Desktop/notice/node_modules/.pnpm/anywhere@1.6.0/node_modules/anywhere/bin/node_modules:/mnt/c/Users/HP/Desktop/notice/node_modules/.pnpm/anywhere@1.6.0/node_modules/anywhere/node_modules:/mnt/c/Users/HP/Desktop/notice/node_modules/.pnpm/anywhere@1.6.0/node_modules:/mnt/c/Users/HP/Desktop/notice/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../anywhere/bin/anywhere" "$@"
else
  exec node  "$basedir/../anywhere/bin/anywhere" "$@"
fi
